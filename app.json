{
  "expo": {
    "name": "field-plotter",
    "slug": "field-plotter",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "leeryorange.fieldplotter"
    },
    "description": "A field marker plotting application I built during the summer of 2019 to help keep track of physical field locations.",
    "githubUrl": "https://github.com/lewissheaffer/field-plotter"
  }
}
